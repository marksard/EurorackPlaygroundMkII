   "source": [
    "import math\n",
    "\n",
    "def lcm(a, b):\n",
    "    return abs(a * b) // math.gcd(a, b)\n",
    "\n",
    "result = 1\n",
    "for i in range(1, 32 + 1):\n",
    "    result = lcm(result, i)\n",
    "\n",
    "print(result)  # 出力:\n",
    "\n",
    "bit_count = result.bit_length()\n",
    "\n",
    "# 結果を出力\n",
    "print(f\"{result} を表現するのに必要なビット数: {bit_count}\")"
   ]
  }
